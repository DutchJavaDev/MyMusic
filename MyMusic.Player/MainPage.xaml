<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             xmlns:toolkit="http://schemas.microsoft.com/dotnet/2022/maui/toolkit"
             x:Class="MyMusic.Player.MainPage"
             Title="">

    <toolkit:DockLayout>
        <BoxView toolkit:DockLayout.DockPosition="Left" Color="White" VerticalOptions="FillAndExpand" HorizontalOptions="EndAndExpand" WidthRequest="2" />
        <Button toolkit:DockLayout.DockPosition="Bottom" Text="Bottom" BackgroundColor="Black" HeightRequest="70" />
        <Button toolkit:DockLayout.DockPosition="None" Text="Center" BackgroundColor="Black" />
    </toolkit:DockLayout>
</ContentPage>
