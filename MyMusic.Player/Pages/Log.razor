@page "/logs"

@* Icon not working lollololoolo *@
@attribute [NavigationInfo("io-home","LOGS")]

<div>
    @if (Logs is not null)
    {
        <div class="row"> 

            @foreach(var log in Logs)
            {
                <div class="col-12">
                    <div><b>@log.Message</b></div>
                    <div>@log.StackTrace</div>
                    <div>@log.Created.ToLongDateString()</div>
                </div>
            }

        </div>
    }
</div>
